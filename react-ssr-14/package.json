{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "this is a react ssr app",
  "main": "index.js",
  "scripts": {
    "rm": "rimraf server && rimraf client",
    "build:server": "webpack --config ./build/webpack.server.prod.config.js",
    "build:client": "webpack --config ./build/webpack.client.prod.config.js",
    "build": "npm run rm && npm run build:server && npm run build:client",
    "start": "node ./server/bundle.js",
    "dev:build:server": "webpack --config ./build/webpack.server.dev.config.js --watch",
    "dev:build:client": "webpack --config ./build/webpack.client.dev.config.js --watch",
    "dev": "npm run rm && npm-run-all --parallel dev:**",
    "start:dev": "nodemon --watch server --exec node ./server/bundle.js",
    "start_dev": "pm2 restart ecosystem.config.js --only react-ssr_dev",
    "start_test": "pm2 restart ecosystem.config.js --only react-ssr_test",
    "start_sim": "pm2 restart ecosystem.config.js --only react-ssr_sim",
    "start_prod": "pm2 restart ecosystem.config.js --only react-ssr_prod",
    "stop_dev": "pm2 stop react-ssr_dev && pm2 delete react-ssr_dev",
    "stop_test": "pm2 stop react-ssr_test && pm2 delete react-ssr_test",
    "stop_sim": "pm2 stop react-ssr_sim && pm2 delete react-ssr_sim",
    "stop_prod": "pm2 stop react-ssr_prod && pm2 delete react-ssr_prod",
    "lint": "eslint --ext .js,.jsx src",
    "lint-fix": "eslint --fix --ext .js,.jsx src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.23.2",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "express-http-proxy": "^1.4.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-mixin": "^5.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "isomorphic-style-loader": "^4.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
