{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "this is a react ssr app",
  "main": "index.js",
  "scripts": {
    "rm": "rimraf server && rimraf client",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js",
    "build": "npm run rm && npm run build:server && npm run build:client",
    "start": "node ./server/bundle.js",
    "dev:build:server": "webpack --config ./build/webpack.server.config.js --watch",
    "dev:build:client": "webpack --config ./build/webpack.client.config.js --watch",
    "dev": "npm run rm && npm-run-all --parallel dev:**",
    "devstart": "nodemon --watch server --exec node ./server/bundle.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "express-http-proxy": "^1.4.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-mixin": "^5.0.0",
    "react-redux": "^5.0.7",
    "react-router-config": "^4.4.0-beta.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "isomorphic-style-loader": "^4.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
